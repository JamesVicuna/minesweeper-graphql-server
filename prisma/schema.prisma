generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model leaderboard {
  id        String   @id @unique @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  user      String
  rows      Int
  columns   Int
  bombs     Int
  time      Int
  createdAt DateTime @default(now())
}
